{
  "version": 2,
  "functions": {
    "server/index.ts": {
      "runtime": "@vercel/node@3.0.26"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/uploads/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ],
  "buildCommand": "vite build",
  "outputDirectory": "dist/public",
  "framework": "vite"
}